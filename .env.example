# Bookeo API (from your GET request - store in .env, never commit)
BOOKEO_API_KEY=your_api_key
BOOKEO_SECRET_KEY=your_secret_key

# Azure SQL (after running deploy script)
AZURE_SQL_SERVER=your-server.database.windows.net
AZURE_SQL_DATABASE=corkandcandles-bookings
AZURE_SQL_USER=sqladmin
AZURE_SQL_PASSWORD=your_secure_password
